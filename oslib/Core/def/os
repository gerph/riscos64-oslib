SWI
   Event_CharInput =
   (  NUMBER &22,
      ENTRY
      (  R0 # 2 "Character input",
         R1 = .Int: buffer,
         R2 = .Byte: b
   )  ),

   Event_VSync =
   (  NUMBER &22,
      ENTRY
      (  R0 # 4 "Vertical sync"
   )  ),

   Event_IntervalTimer =
   (  NUMBER &22,
      ENTRY
      (  R0 # 5 "Interval timer"
   )  ),

   Event_Escape =
   (  NUMBER &22,
      ENTRY
      (  R0 # 6 "Escape"
   )  ),

   Event_User =
   (  NUMBER &22,
      ENTRY
      (  R0 # 9 "User event - reserved"
      ),
      ABSENT
   ),

   Event_MouseTransition =
   (  NUMBER &22,
      ENTRY
      (  R0 # 10 "Mouse button up/down",
         R1 = .Int: x,
         R2 = .Int: y,
         R3 = .Bits: state,
         R4 = OS_T: t
      )
   ),

   Event_KeyTransition =
   (  NUMBER &22,
      ENTRY
      (  R0 # 11 "Key up/down",
         R1 = .Bool: down,
         R2 = .Int: key,
         R3 = .Int: keyboard_id
   )  ),

   Event_PointerScroll =
