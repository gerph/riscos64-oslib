   (  NUMBER 0xE "Reads a line from the input stream, using the given echo 
            character",
      ENTRY
      (  R0 # &40000000,
         R0 | .Ref .String: buffer,
         R1 = .Int: size,
         R2 = .Char: min_char,
         R3 = .Char: max_char,
         R4 = .Char: echo
      ),
      EXIT
      (  R0?,
         R1 = .Int: used,
         R2?,
         R3?,
         FLAGS!
   )  );

