         R3 = .Ref .Data: buffer,
         R9 # &1E "OS_ChangeEnvironment vector"
      ),
      EXIT
      (  R1! -> .Asm: old_handler,
         R2 = .Ref .Data: old_handle, //should be int, like other handles
         R3 = .Ref .Data: old_buffer
   )  );

